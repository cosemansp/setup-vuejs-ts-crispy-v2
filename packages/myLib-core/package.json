{
  "name": "@me/mylib-core",
  "version": "0.1.0",
  "description": "Seed example of TypeScript VueJS library",
  "private": true,
  "author": "Peter Cosemans",
  "scripts": {
    "build": "npm run build:cjs && npm run build:es && npm run build:umd:dev && npm run build:umd:prod",
    "build:cjs": "rollup -c --environment TARGET:cjs",
    "build:es": "rollup -c --environment TARGET:esm",
    "build:umd:dev": "rollup -c --environment TARGET:umd-dev",
    "build:umd:prod": "rollup -c --environment TARGET:umd-prod",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o docs/dist/stories",
    "lint": "node scripts/tslint-vue.js",
    "lint:prod": "NODE_ENV=production node scripts/tslint-vue.js",
    "format": "prettier --write **/*.{ts,vue,json}",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watch",
    "typings": "vuetype src/components",
    "cleanup": "rimraf ./dist && rimraf .tmp"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "main": "dist/mylib-core.common.js",
  "module": "dist/mylib-core.esm.js",
  "unpkg": "dist/mylib-core.min.js",
  "jsdelivr": "dist/mylib-core.min.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "date-fns": "^1.29.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.0",
    "@storybook/addon-centered": "3.4.0",
    "@storybook/addon-knobs": "3.4.0",
    "@storybook/addon-links": "3.4.0",
    "@storybook/addon-notes": "3.4.0",
    "@storybook/addon-options": "3.4.0",
    "@storybook/addon-storysource": "3.4.0",
    "@storybook/addon-viewport": "3.4.0",
    "@storybook/addons": "3.4.0",
    "@storybook/react": "^3.3.15",
    "@storybook/vue": "3.4.0",
    "@types/jest": "^22.0.1",
    "@types/storybook__addon-actions": "^3.0.3",
    "@types/storybook__addon-links": "^3.3.0",
    "@types/storybook__addon-notes": "^3.3.1",
    "@types/storybook__vue": "^3.3.0",
    "@vue/cli-plugin-typescript": "^3.0.0-beta.6",
    "@vue/test-utils": "^1.0.0-beta.13",
    "babel-core": "^6.26.0",
    "babel-preset-vue": "^2.0.2",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "jest": "^22.4.3",
    "jest-serializer-vue": "^1.0.0",
    "lodash": "^4.17.5",
    "parse5": "^4.0.0",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-license": "^0.6.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.12.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-plugin-vue": "^3.0.0",
    "ts-jest": "^22.0.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.10.0",
    "uglify-es": "^3.3.9",
    "vue": "^2.5.16",
    "vue-jest": "^2.3.0",
    "vue-loader": "14.2.1",
    "vue-property-decorator": "^6.0.0",
    "vue-template-compiler": "^2.5.16",
    "vuetype": "^0.3.2"
  },
  "peerDependencies": {
    "vue": "2.x",
    "vue-template-compiler": "^2.x"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
