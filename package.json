{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --mode=development",
    "build:prod": "cross-env ANALYZE=true vue-cli-service build",
    "test": "jest --verbose",
    "lint": "vue-cli-service lint && npm run stylelint",
    "stylelint": "stylelint --fix \"src/**/*.vue\" \"src/**/*.scss\"",
    "format": "prettier --write \"**/*.{ts,json,css,scss,vue,md}\"",
    "test:watch": "jest --verbose --watch",
    "cleanup": "rimraf ./dist",
    "i18n:extract": "node ./scripts/extract.js -o ./i18n/template.pot -s ./src",
    "i18n:compile": "node ./scripts/compile.js --output ./src/locales ./i18n/nl.po ./i18n/en.po",
    "i18n:pseudo": "node ./scripts/compile.js --output ./src/locales --pseudo ./i18n/template.pot en",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "gitHooks": {
    "pre-commit": "PRE_COMMIT=true lint-staged"
  },
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.0.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "3.3.7",
    "loglevel": "^1.6.1",
    "luxon": "^1.0.0",
    "nprogress": "^0.2.0",
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.13",
    "vue-i18n": "^7.8.0",
    "vue-meta": "^1.5.0",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.0",
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.2.0",
    "@types/jest": "^22.0.1",
    "@types/loglevel": "^1.5.3",
    "@types/webpack": "^4.4.0",
    "@types/webpack-env": "^1.13.6",
    "@vue/cli-plugin-babel": "^3.0.0-beta.16",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.16",
    "@vue/cli-plugin-pwa": "^3.0.0-beta.16",
    "@vue/cli-plugin-typescript": "^3.0.0-beta.16",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.16",
    "@vue/cli-service": "^3.0.0-beta.16",
    "@vue/eslint-config-airbnb": "^3.0.0-beta.16",
    "@vue/eslint-config-typescript": "^3.0.0-beta.16",
    "@vue/test-utils": "^1.0.0-beta.18",
    "axios-mock-adapter": "^1.14.1",
    "babel-core": "7.0.0-bridge.0",
    "cross-env": "^5.1.4",
    "eslint-config-prettier": "^2.9.0",
    "gettext-extractor": "^3.3.0",
    "gettext-parser": "^1.3.1",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "imagemin-lint-staged": "^0.3.0",
    "intl": "^1.2.5",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.4",
    "node-sass": "^4.9.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "shelljs": "^0.8.2",
    "stylelint": "^8.4.0",
    "stylelint-config-css-modules": "^1.2.0",
    "stylelint-config-prettier": "^3.2.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-scss": "^3.0.0",
    "ts-jest": "^22.0.1",
    "vue-cli-plugin-i18n": "^0.2.0",
    "vue-jest": "^2.3.0",
    "vue-server-renderer": "^2.5.16",
    "vue-storybook": "^0.3.0",
    "vue-template-compiler": "^2.5.16",
    "webpack-bundle-analyzer": "^2.13.1",
    "yargs": "^11.0.0"
  },
  "browserslist": ["> 1%", "last 2 versions", "not ie <= 10"]
}
